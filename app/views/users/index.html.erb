

<ul>

  <% @users.each do |user| %>
  <li><%= user.name%>
    <br>

    
       
       <%= link_to 'Follow', followships_path(following_id: user), method: :post, class: "btn btn-info" %>
       <%= link_to 'Unfollow', followship_path(user), method: :delete, class: "btn btn-info" %>
     
  </li>
     
<% end %>
  
</ul>




 